<div class='basic_item'>
	<h2><%= link_to "bh# #{item.bhsku}", item_path(item) %></h2>
	<p>
		<b>mfr#</b> <%= item.mfrsku %>
		<br>
		<b>updated_at:</b> <%= item.updated_at %>  || <b>created at:</b> <%= item.created_at %>
		<br>
		<%= link_to 'edit item', edit_item_path(item) %>
	</p>

	<%= render partial: 'tags/tags', locals: {:taggable_item => item} %>
</div>
